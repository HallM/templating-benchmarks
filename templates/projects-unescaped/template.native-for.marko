<html>
    <head>
        <title>$!{data.title}</title>
    </head>
    <body>
        <p>$!{data.text}</p>
        <for each="project in data.projects" for-loop="true">
            <a href="$!{project.url}">$!{project.name}</a>
            <p>$!{project.description}</p>
        </for>
        <if test="empty(data.projects)">
            No projects
        </if>
    </body>
</html>