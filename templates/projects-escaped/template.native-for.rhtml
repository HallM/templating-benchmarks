<html>
    <head>
        <title>${data.title}</title>
    </head>
    <body>
        <p>${data.text}</p>
        <c-for each="project in data.projects" for-loop="true">
            <a href="${project.url}">${project.name}</a>
            <p>${project.description}</p>
        </c-for>
        <c-if test="empty(data.projects)">
            No projects
        </c-if>
    </body>
</html>